{
  "name": "@fasttime/gulp-lint",
  "version": "0.32.0",
  "description": "Gulp plugin for linting with fasttime presets.",
  "keywords": [
    "eslint",
    "gulp",
    "gulp-plugin",
    "gulpplugin",
    "lint",
    "linter",
    "validate",
    "validation",
    "validator"
  ],
  "homepage": "https://github.com/fasttime/gulp-lint",
  "bugs": "https://github.com/fasttime/gulp-lint/issues",
  "license": "ISC",
  "author": "Francesco Trotta <ft@fasttime.org> (https://github.com/fasttime)",
  "files": [
    "index.{d.ts,js}",
    "lib/*.js"
  ],
  "types": "index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fasttime/gulp-lint.git"
  },
  "scripts": {
    "build": "npm install && gulp",
    "test": "mocha --check-leaks test/test.js"
  },
  "dependencies": {
    "@fasttime/eslint-plugin": "0.5",
    "@types/eslint": "7",
    "@typescript-eslint/eslint-plugin": "4.2",
    "@typescript-eslint/parser": "4.2",
    "eslint": "7.9",
    "eslint-plugin-node": "11",
    "gherkin": "9",
    "gulp-eslint7": "0.3",
    "gulp-tap": "2",
    "merge-stream": "2",
    "plugin-error": "1",
    "semver": "7",
    "through2": "4",
    "typescript": "4.0",
    "vinyl-fs": "3"
  },
  "devDependencies": {
    "ansi-colors": "latest",
    "c8": "latest",
    "gulp": "latest",
    "mocha": "latest",
    "postrequire": "latest",
    "vinyl-buffer": "latest",
    "vinyl-source-stream": "latest"
  },
  "peerDependencies": {
    "@types/node": "*"
  }
}
