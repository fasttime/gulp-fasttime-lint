{
  "name": "@fasttime/gulp-lint",
  "version": "0.29.2",
  "description": "Gulp plugin for linting with fasttime presets.",
  "keywords": [
    "eslint",
    "gulp",
    "gulp-plugin",
    "gulpplugin",
    "lint",
    "linter",
    "validate",
    "validation",
    "validator"
  ],
  "homepage": "https://github.com/fasttime/gulp-lint",
  "bugs": "https://github.com/fasttime/gulp-lint/issues",
  "license": "ISC",
  "author": "Francesco Trotta <ft@fasttime.org> (https://github.com/fasttime)",
  "files": [
    "index.{d.ts,js}",
    "lib/*.js"
  ],
  "types": "index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fasttime/gulp-lint.git"
  },
  "scripts": {
    "build": "npm install && gulp",
    "test": "mocha --check-leaks test/test.js"
  },
  "dependencies": {
    "@fasttime/eslint-plugin": "0.5",
    "@types/eslint": "7",
    "@typescript-eslint/eslint-plugin": "3.6",
    "@typescript-eslint/parser": "3.6",
    "eslint": "7.4",
    "eslint-plugin-node": "11.1",
    "gherkin": "latest",
    "gulp-eslint7": "latest",
    "gulp-tap": "latest",
    "merge-stream": "latest",
    "plugin-error": "latest",
    "semver": "latest",
    "through2": "latest",
    "typescript": "latest",
    "vinyl-fs": "latest"
  },
  "devDependencies": {
    "ansi-colors": "~4.1.1",
    "c8": "~7.2.0",
    "gulp": "~4.0.2",
    "mocha": "~8.0.1",
    "postrequire": "~1.0.0",
    "vinyl-buffer": "~1.0.1",
    "vinyl-source-stream": "~2.0.0"
  },
  "peerDependencies": {
    "@types/node": "*"
  }
}
