{
  "name": "@fasttime/gulp-lint",
  "version": "0.27.1",
  "description": "Gulp plugin for linting with fasttime presets.",
  "keywords": [
    "eslint",
    "gulp",
    "gulp-plugin",
    "gulpplugin",
    "lint",
    "linter",
    "validate",
    "validation",
    "validator"
  ],
  "homepage": "https://github.com/fasttime/gulp-lint",
  "bugs": "https://github.com/fasttime/gulp-lint/issues",
  "license": "ISC",
  "author": "Francesco Trotta <ft@fasttime.org> (https://github.com/fasttime)",
  "files": [
    "index.{d.ts,js}",
    "lib/*.js"
  ],
  "types": "index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fasttime/gulp-lint.git"
  },
  "scripts": {
    "build": "npm install && gulp",
    "test": "mocha --check-leaks test/test.js"
  },
  "dependencies": {
    "@fasttime/eslint-plugin": "~0.4.1",
    "@types/eslint": "6.8.0",
    "@typescript-eslint/eslint-plugin": "~2.33.0",
    "@typescript-eslint/parser": "~2.33.0",
    "gherkin": "~9.0.0",
    "gulp-eslint": "~6.0.0",
    "gulp-tap": "~2.0.0",
    "merge-stream": "~2.0.0",
    "plugin-error": "~1.0.1",
    "semver": "~7.3.2",
    "through2": "~3.0.1",
    "typescript": "~3.9.2",
    "vinyl-fs": "~3.0.3"
  },
  "devDependencies": {
    "ansi-colors": "~4.1.1",
    "c8": "~7.1.2",
    "gulp": "~4.0.2",
    "mocha": "~7.1.2",
    "postrequire": "~1.0.0",
    "vinyl-buffer": "~1.0.1",
    "vinyl-source-stream": "~2.0.0"
  },
  "peerDependencies": {
    "@types/node": "*",
    "eslint": ">=6.2.0"
  }
}
