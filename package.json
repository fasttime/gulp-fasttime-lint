{
  "name": "@fasttime/gulp-lint",
  "version": "0.29.0",
  "description": "Gulp plugin for linting with fasttime presets.",
  "keywords": [
    "eslint",
    "gulp",
    "gulp-plugin",
    "gulpplugin",
    "lint",
    "linter",
    "validate",
    "validation",
    "validator"
  ],
  "homepage": "https://github.com/fasttime/gulp-lint",
  "bugs": "https://github.com/fasttime/gulp-lint/issues",
  "license": "ISC",
  "author": "Francesco Trotta <ft@fasttime.org> (https://github.com/fasttime)",
  "files": [
    "index.{d.ts,js}",
    "lib/*.js"
  ],
  "types": "index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fasttime/gulp-lint.git"
  },
  "scripts": {
    "build": "npm install && gulp",
    "test": "mocha --check-leaks test/test.js"
  },
  "dependencies": {
    "@fasttime/eslint-plugin": "~0.5.0",
    "@types/eslint": "7.2.0",
    "@typescript-eslint/eslint-plugin": "~3.5.0",
    "@typescript-eslint/parser": "~3.5.0",
    "eslint": "~7.3.1",
    "eslint-plugin-node": "~11.1.0",
    "gherkin": "~9.0.0",
    "gulp-eslint7": "~0.2.1",
    "gulp-tap": "~2.0.0",
    "merge-stream": "~2.0.0",
    "plugin-error": "~1.0.1",
    "semver": "~7.3.2",
    "through2": "~4.0.2",
    "typescript": "~3.9.5",
    "vinyl-fs": "~3.0.3"
  },
  "devDependencies": {
    "ansi-colors": "~4.1.1",
    "c8": "~7.2.0",
    "gulp": "~4.0.2",
    "mocha": "~8.0.1",
    "postrequire": "~1.0.0",
    "vinyl-buffer": "~1.0.1",
    "vinyl-source-stream": "~2.0.0"
  },
  "peerDependencies": {
    "@types/node": "*"
  }
}
